{
    "schemaVersion": 1,
    "id": "componentviewer",
    "version": "${version}",
    "name": "Component Viewer",
    "description": "'Component Viewer' is a mod for Minecraft Java Edition that allows you to view and copy components of any item stack.",
    "authors": [
        "fixyldev"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/component-viewer",
        "issues": "https://github.com/fixyldev/ComponentViewer/issues",
        "sources": "https://github.com/fixyldev/ComponentViewer"
    },
    "license": "MIT",
    "icon": "assets/componentviewer/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "dev.fixyl.componentviewer.ComponentViewer"
        ],
        "modmenu": [
            "dev.fixyl.componentviewer.modmenu.ModMenu"
        ]
    },
    "mixins": [
        "componentviewer.mixins.json"
    ],
    "depends": {
        "fabric-api": ">=0.100.1",
        "fabricloader": ">=0.15.6",
        "minecraft": ">=1.21 <1.21.5"
    }
}
